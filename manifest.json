{
  "name": "osu! beatmaps",
  "version": "0.0.1",
  "description": "osu! beatmap previewer",
  "permissions": [
    "tabs",
    "*://*.ppy.sh/*"
  ],
  "browser_action": {
    "default_title": "beatmap preview",
    "default_popup": "src/popup.html",
    "default_icon": {
      "128": "src/img/icon.png"
    }
  },
  "icons": {
    "128": "src/img/icon.png"
  },
  "manifest_version": 2
}
